@{
    ViewData["Title"] = "Home Page";
}
@if (ViewBag.msg != null)
{
    <h1>@ViewBag.msg</h1>
}
<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (Table item in ViewBag.data)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>