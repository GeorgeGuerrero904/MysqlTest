@{
    ViewData["Title"] = "Home Page";
}
@if (ViewBag.msg != null)
{
    <h1>@ViewBag.msg</h1>
}
<table class="table table-striped">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td></td>
            
        </tr>
    </thead>
    <tbody>
        @foreach (Table item in ViewBag.data)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@Html.ActionLink("Edit", "Edit", "Home", new { id = item.Id }, new {@class = "btn btn-primary"})</td>
            </tr>
        }
    </tbody>
</table>